{
  "name": "InspiredbyGithub",
  "type": "light",
  "colors": {
    "focusBorder": "#fafbfc",
    "editor.background": "#ffffff",
    "editor.foreground": "#24292e",
    "editor.lineHighlightBackground": "#fffbdd",
    "editorIndentGuide.background": "#eeeeee",
    "editorLineNumber.foreground": "#cccccc",
    "editorBracketMatch.background": "#f1f8ff",
    "editorBracketMatch.border": "#c8e1ff",
    "statusBar.foreground": "#24292e",
    "statusBar.noFolderForeground": "#24292e",
    "statusBar.background": "#fafbfc",
    "statusBar.noFolderBackground": "#fafbfc",
    "statusBar.border": "#dddddd",
    "activityBar.background": "#24292e",
    "sideBar.background": "#fafbfc",
    "sideBar.foreground": "#586069",
    "sideBar.border": "#dddddd",
    "sideBarTitle.foreground": "#24292e",
    "sideBarSectionHeader.background": "#fafbfc",
    "sideBarSectionHeader.foreground": "#24292e",
    "notification.background": "#54a3ff",
    "statusBar.debuggingBackground": "#fafbfc",
    "statusBar.debuggingForeground": "#24292e"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#24292eff",
        "background": "#ffffffff"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "variable.language.this",
        "support.type.primitive",
        "support.function",
        "support.variable.dom",
        "support.variable.property",
        "support.variable.property",
        "meta.property-name",
        "meta.property-value",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "meta.function-call",
        "entity.other.attribute-name",
        "entity.name.function.shell"
      ],
      "settings": {
        "foreground": "#6f42c1"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#22863a"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316bcd"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": ["variable.language.php", "punctuation.definition.variable.php"],
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "scope": ["storage"],
      "settings": {
        "foreground": "#d73a49"
      }
    },
    {
      "scope": ["variable.language.this.php"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#24292e"
      }
    }
  ]
}
